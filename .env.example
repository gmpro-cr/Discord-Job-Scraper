# Job Search Agent - Environment Variables
# Copy this file to .env and fill in your values.
# These override any values saved in user_preferences.json.

# Gmail SMTP for email digests
GMAIL_ADDRESS=
GMAIL_APP_PASSWORD=

# Email recipient for digests
EMAIL_RECIPIENT=

# Discord notifications
DISCORD_WEBHOOK_URL=
DISCORD_BOT_TOKEN=
DISCORD_MIN_SCORE=65

# Apollo.io contact enrichment
APOLLO_API_KEY=

# OpenRouter API (access to multiple LLMs)
# Get your key at https://openrouter.ai/keys
OPENROUTER_API_KEY=
